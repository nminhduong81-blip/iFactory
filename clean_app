import shutil, pathlib
for p in ('build', 'dist'):
    shutil.rmtree(p, ignore_errors=True)
for p in pathlib.Path('.').rglob('__pycache__'):
    shutil.rmtree(p, ignore_errors=True)
for f in pathlib.Path('.').rglob('*.pyc'):
    try: f.unlink()
    except: pass
print('Cleaned build/dist/__pycache__')